<?xml version="1.0" encoding="UTF-8"?>
  <workout_file>
      <author>Pierre-Alexandre St-Jean</author>
      <n>W12-T: Final FTP Test</n>
      <description>Final FTP test to measure your improvement. Compare to Week 1!</description>
      <sportType>bike</sportType>
      <tags>
          <tag name="Week 12"/>
        <tag name="FTP Test"/>
        <tag name="Final Assessment"/>
      </tags>
      <workout>
          <Warmup Duration="300" PowerLow="0.5" PowerHigh="0.6">
            <textevent timeoffset="10" message="Final FTP test warmup"/>
        </Warmup>
        <Warmup Duration="600" PowerLow="0.6" PowerHigh="0.75">
        </Warmup>
        <SteadyState Duration="300" Power="0.5">
        </SteadyState>
        <SteadyState Duration="60" Power="1.1">
            <textevent timeoffset="10" message="Opener 1/3 - feeling strong!"/>
        </SteadyState>
        <SteadyState Duration="120" Power="0.5">
        </SteadyState>
        <SteadyState Duration="60" Power="1.1">
            <textevent timeoffset="10" message="Opener 2/3"/>
        </SteadyState>
        <SteadyState Duration="120" Power="0.5">
        </SteadyState>
        <SteadyState Duration="60" Power="1.1">
            <textevent timeoffset="10" message="Final opener - show your improvement!"/>
        </SteadyState>
        <SteadyState Duration="300" Power="0.5">
            <textevent timeoffset="240" message="Time to see your gains!"/>
        </SteadyState>
        <SteadyState Duration="1200" Power="1">
            <textevent timeoffset="10" message="FINAL FTP TEST! Show what you've built!"/>
            <textevent timeoffset="300" message="5 min - settle in strong"/>
            <textevent timeoffset="600" message="Halfway - how does this feel vs Week 1?"/>
            <textevent timeoffset="900" message="5 min left - time to prove your fitness!"/>
            <textevent timeoffset="1080" message="Final 2 min - everything you've gained!"/>
            <textevent timeoffset="1170" message="30 seconds - FINISH STRONG!"/>
        </SteadyState>
        <Cooldown Duration="900" PowerHigh="0.5" PowerLow="0.3">
            <textevent timeoffset="10" message="CONGRATULATIONS! 12 weeks complete!"/>
            <textevent timeoffset="300" message="Compare to Week 1 - celebrate your progress!"/>
        </Cooldown>
      </workout>
  </workout_file>